<%= simple_form_for @venue, :html => { :class => "form-horizontal" } do |f| %>
  <fieldset>
    <%= f.input :venue_name %>
    <%= f.association :branch, label_method: :cabang  , value_method: :id %>
    <%= f.input :city %>
    <%= f.input :from_period, :as => :date_picker %>
    <%= f.input :to_period, :as => :date_picker %>

    <div class="form-actions">
      <%= f.submit nil, :class => 'btn btn-primary' %>
      <%= link_to 'Cancel', venues_path, :class => 'btn' %>
    </div>
  </fieldset>
<% end %>
