<%- model_class = Venue -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:venue_name) %>:</strong></dt>
  <dd><%= @venue.venue_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:city) %>:</strong></dt>
  <dd><%= @venue.city %></dd>
  <dt><strong><%= model_class.human_attribute_name(:branch_id) %>:</strong></dt>
  <dd><%= @venue.branch_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:from_period) %>:</strong></dt>
  <dd><%= @venue.from_period %></dd>
  <dt><strong><%= model_class.human_attribute_name(:to_period) %>:</strong></dt>
  <dd><%= @venue.to_period %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              venues_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_venue_path(@venue), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              venue_path(@venue),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
